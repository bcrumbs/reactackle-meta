{
  "namespace": "Reactackle",
  "globalStyle": true,
  "containerStyle": {
    "height": "100%",
    "min-height": "100vh",
    "width": "100vw",
    "min-width": "0"
  },
  "loaders": {
    "\\.jsx?$": [
      {
        "loader": "babel-loader",
        "options": {
          "presets": ["es2015", "react"],
          "plugins": ["transform-object-rest-spread", "styled-components"]
        }
      }
    ],
    "\\.(jpe?g|png|gif|svg)$": [
      {
        "loader": "file-loader",
        "options": {
          "hash": "sha512",
          "digest": "hex",
          "name": "[name].[ext]"
        }
      },
      {
        "loader": "image-webpack-loader",
        "options": {
          "bypassOnDebug": true,
          "optimizationLevel": "7",
          "interlaced": "false"
        }
      }
    ],
    "\\.woff2?(\\?v=[0-9]\\.[0-9]\\.[0-9])?": [
      {
        "loader": "url-loader",
        "options": {
          "limit": "10000",
          "mimetype": "application/font-woff"
        }
      }
    ],
    "\\.(ttf|eot|svg)(\\?v=[0-9]\\.[0-9]\\.[0-9])?$": [
      "file-loader"
    ]
  },
  "import": [],
  "componentGroups": {
    "app": {
      "textKey": "group_app",
      "descriptionTextKey": "group_app_desc"
    },
    "blocks": {
      "textKey": "group_blocks",
      "descriptionTextKey": "group_blocks_desc"
    },
